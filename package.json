  "bugs": {
    "url": "https://github.com/podio/podio-js/issues"
  },
  "homepage": "https://github.com/podio/podio-js",
  "devDependencies": {
    "coveralls": "2.11.2",
    "istanbul": "0.3.15",
    "jasmine-node": "1.14.5",
    "sinon": "1.15.3"
  },
  "dependencies": {
    "URIjs": "1.15.1",
    "es6-promise": "2.3.0",
    "lodash": "3.9.3",
    "superagent": "1.2.0"
